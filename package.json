{
  "name": "rascal-art-website",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "lsof -ti:3001 | xargs kill -9 2>/dev/null || true && next dev -p 3001",
    "dev:r2": "lsof -ti:3001 | xargs kill -9 2>/dev/null || true && next dev -p 3001",
    "build": "node scripts/validate-data.js && NODE_ENV=production node scripts/prebuild-clean.js && next build && (if [ -z \"$CI\" ] && command -v lsof >/dev/null 2>&1; then bash scripts/auto-restart-dev.sh; fi)",
    "start": "next start -p 3001",
    "lint": "next lint",
    "validate-data": "node scripts/validate-data.js",
    "generate-dailies": "node scripts/generate-dailies.js",
    "fetch-collection": "node scripts/fetch-collection.js",
    "verify-dev": "bash scripts/verify-dev-server.sh",
    "post-change": "bash scripts/post-change-check.sh"
  },
  "dependencies": {
    "ethers": "^6.16.0",
    "framer-motion": "^11.0.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.33",
    "puppeteer": "^24.36.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  }
}
